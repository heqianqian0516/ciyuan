<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relative_layout"
    android:orientation="vertical" android:layout_width="match_parent"
    android:background="#858585"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/home_back"
        android:layout_width="31dp"
        android:layout_height="31dp"
        android:src="@mipmap/home_back"
        android:layout_marginTop="170dp"
        android:layout_marginLeft="293dp"
        />
    <ImageView
        android:id="@+id/home_sign"
        android:layout_width="275dp"
        android:layout_height="304dp"
        android:src="@mipmap/home_sign_gifts"
        android:layout_marginTop="165dp"
        android:layout_marginLeft="67dp"
        />
     <ImageView
         android:id="@+id/home_check_mmediately"
         android:layout_width="132dp"
         android:layout_height="32dp"
         android:src="@mipmap/home_check_in_immediately"
         android:layout_below="@+id/home_sign"
         android:layout_marginLeft="122dp"
         android:layout_marginTop="15dp"
         />
</RelativeLayout>